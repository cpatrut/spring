package ro.catalin.aspect;

import org.aspectj.lang.annotation.Aspect;
import org.aspectj.lang.annotation.Before;
import org.aspectj.lang.annotation.Pointcut;

@Aspect
public class LoggingAspect {
	
	@Before("execution(public String ro.catalin.model.Circle.getName() )")
	public void LoggingAdvice() {
		System.out.println("advice run. Get method called");
	}

	@Before("execution(public ro.catalin.model.Circle* get*() )")
	public void LoggingAdvice1() {
		System.out.println("advice 1run. Get method called");
	}
	@Before("execution(public * get*() )")
	public void LoggingAdvice2() {
		System.out.println("advice star run. Get method called");
	}
	@Before("execution(* get*())")
	public void secondAdvice(){
		System.out.println("Second advice execiuted");
	}
	@Pointcut("execution(* get())")
	public void allGetters(){
		
	}
	}
	
}
